{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard",
        "plugin:vue/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "camelcase": "off",
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ],
        "no-console": "off",
        "vue/require-default-prop": "off",
        "vue/attribute-hyphenation": "off"
    }
}